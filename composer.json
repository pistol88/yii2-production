{
    "name": "dvizh/yii2-production",
    "description": "Module for Yii2 who provide constructor of products for system",
    "keywords": ["yii2", "yii 2", "application", "production", "manufacturer", "module", "dvizh"],
    "homepage": "https://github.com/dvizh/",
    "type": "yii2-extension",
    "license": "Apache",
    "support": {
        "source": "https://github.com/dvizh/yii2-production"
    },
    "authors": [
        {
            "name": "Ilya Pistol",
            "email": "admin@nethammer.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-widget-select2": "*",
        "voskobovich/yii2-linker-behavior": "^4.0"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "dvizh\\production\\": "src\\"
        }
    },
    "extra": {
        "bootstrap": "dvizh\\production\\Bootstrap"
    }
}
